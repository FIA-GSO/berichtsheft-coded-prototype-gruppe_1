{
  "name": "prototype",
  "version": "1.0.0",
  "description": "Ein Prototyp f√ºr eine Berichthefte-App",
  "main": "index.py",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:pug": "pug src --out dist",
    "build:css": "copyfiles -f src/styles/**/*.css dist",
    "build": "npm-run-all build:pug build:css",
    "watch:pug": "pug src --out dist --watch",
    "watch:css": "npm run build:css && chokidar \"src/styles/**/*.css\" -c \"copyfiles -f src/styles/**/*.css dist\"",
    "serve": "live-server dist",
    "start": "npm-run-all --parallel watch:pug watch:css serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FIA-GSO/berichtsheft-coded-prototype-gruppe_1.git"
