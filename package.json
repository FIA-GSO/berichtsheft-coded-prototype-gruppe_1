  "bugs": {
    "url": "https://github.com/FIA-GSO/berichtsheft-coded-prototype-gruppe_1/issues"
  },
  "homepage": "https://github.com/FIA-GSO/berichtsheft-coded-prototype-gruppe_1#readme",
  "dependencies": {
    "python3": "^0.0.1"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "copyfiles": "^2.4.1",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5"
  }
}
