{
  "name": "prototype",
  "version": "1.0.0",
  "description": "Ein Prototyp f√ºr eine Berichthefte-App",
  "main": "index.py",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:html": "copyfiles -f src/**/*.html dist/",
    "build:css": "copyfiles -f src/styles/**/*.css dist/",
    "build": "npm-run-all build:html build:css",
    "watch:html": "npm run build:html && chokidar \"src/**/*.html\" -c \"npm run build:html\"",
    "watch:css": "npm run build:css && chokidar \"src/styles/**/*.css\" -c \"npm run build:css\"",
    "serve": "live-server dist",
    "start": "npm-run-all --parallel watch:html watch:css serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FIA-GSO/berichtsheft-coded-prototype-gruppe_1.git"
  },
  "author": "",
