  "dependencies": {
    "pug": "^3.0.3",
    "python3": "^0.0.1"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "copyfiles": "^2.4.1",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6"
  }
